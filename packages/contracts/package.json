{
  "name": "@injectivelabs/contracts",
  "description": "Contracts wrapper that can be reused throughout Injective's projects.",
  "version": "0.0.24",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "~contracts": "dist"
  },
  "scripts": {
    "postinstall": "shx mkdir -p dist && link-module-alias",
    "build": "tsc --build tsconfig.build.json && link-module-alias",
    "build:watch": "tsc --build -w tsconfig.build.json && link-module-alias",
    "clean": "tsc --build tsconfig.build.json --clean && shx rm -rf coverage *.log junit.xml dist && jest --clearCache",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/unit",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --reporters='jest-junit'",
    "coverage": "jest --coverage",
    "coverage:show": "live-server coverage",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@injectivelabs/exceptions": "^0.0.18",
    "@injectivelabs/ts-types": "^0.0.15",
    "@injectivelabs/tx-utils": "^0.0.3",
    "@injectivelabs/utils": "^0.0.22",
    "@injectivelabs/web3-contract-typings": "^0.1.12",
    "@injectivelabs/web3-strategy": "^0.0.25",
    "@types/ethereumjs-util": "^6.1.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "ethereum-types": "^3.4.0",
    "link-module-alias": "^1.2.0",
    "serve": "^11.2.0",
    "shx": "^0.3.2",
    "ts-jest": "^24.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.4.3",
    "web3": "^1.3.4",
    "web3-core-subscriptions": "^1.3.4",
    "web3-utils": "^1.3.4"
  },
  "gitHead": "05eb3472375cb5f1d46ba545222f1edafedbda75"
}
