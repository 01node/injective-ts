{
  "name": "@injectivelabs/sdk-ts",
  "description": "SDK in TypeScript for building Injective applications in a Node environment.",
  "version": "0.0.39",
  "license": "MIT",
  "author": {
    "name": "Bojan Angjelkoski",
    "email": "bojan@injectivelabs.org"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "~sdk-ts": "dist"
  },
  "exports": {
    ".": "./dist/index.js",
    "./local": "./dist/local/index.js",
    "./utils": "./dist/utils/index.js",
    "./types": "./dist/types/index.js",
    "./classes": "./dist/classes/index.js",
    "./core": "./dist/core/index.js",
    "./client": "./dist/client/index.js",
    "./chain-grpc-client": "./dist/client/chain/ChainGrpcClient.js",
    "./chain-rest-client": "./dist/client/chain/ChainRestClient.js",
    "./exchange-grpc-client": "./dist/client/exchange/ExchangeGrpcClient.js",
    "./exchange-rest-client": "./dist/client/exchange/ExchangeRestClient.js",
    "./exchange-grpc-stream-client": "./dist/client/exchange/ExchangeGrpcStreamClient.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "local": [
        "./dist/local/index.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "classes": [
        "./dist/classes/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ],
      "core": [
        "./dist/core/index.d.ts"
      ],
      "client": [
        "./dist/client/index.d.ts"
      ],
      "chain-grpc-client": [
        "./dist/client/chain/ChainGrpcClient.d.ts"
      ],
      "chain-rest-client": [
        "./dist/client/chain/ChainRestClient.d.ts"
      ],
      "exchange-grpc-client": [
        "./dist/client/exchange/ExchangeGrpcClient.d.ts"
      ],
      "exchange-rest-client": [
        "./dist/client/exchange/ExchangeRestClient.d.ts"
      ],
      "exchange-grpc-stream-client": [
        "./dist/client/exchange/ExchangeGrpcStreamClient.d.ts"
      ]
    }
  },
  "scripts": {
    "postinstall": "shx mkdir -p dist && link-module-alias",
    "build:watch": "tsc --build -w tsconfig.build.json && link-module-alias",
    "build": "tsc --build tsconfig.build.json && link-module-alias",
    "clean": "tsc --build tsconfig.build.json --clean && shx rm -rf coverage *.log junit.xml dist && jest --clearCache",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/unit",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --reporters='jest-junit'",
    "coverage": "jest --coverage",
    "coverage:show": "live-server coverage",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@improbable-eng/grpc-web-node-http-transport": "^0.15.0",
    "@injectivelabs/chain-api": "^1.7.1",
    "@injectivelabs/exceptions": "^0.5.3",
    "@injectivelabs/exchange-api": "^2.2.70",
    "@injectivelabs/networks": "^0.5.7",
    "@injectivelabs/token-metadata": "^0.5.22",
    "@injectivelabs/ts-types": "^0.5.2",
    "@injectivelabs/tx-utils": "^0.5.2",
    "@injectivelabs/utils": "^0.5.4",
    "@types/google-protobuf": "^3.15.5",
    "bech32": "^2.0.0",
    "bip32": "^3.0.1",
    "bip39": "^3.0.4",
    "ethereumjs-util": "^7.1.4",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^5.6.4",
    "graphql": "^16.3.0",
    "js-sha3": "^0.8.0",
    "keccak256": "^1.0.6",
    "link-module-alias": "^1.2.0",
    "secp256k1": "^4.0.3",
    "shx": "^0.3.2",
    "snakecase-keys": "^5.4.1",
    "tiny-secp256k1": "^2.2.1"
  },
  "gitHead": "301db22f30cc88ffaf3f32731774386f172f6ee4"
}
